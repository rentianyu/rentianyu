      <span class="s">&#34;token&#34;</span><span class="o">:</span> <span class="s">&#34;私人令牌&#34;</span>
    <span class="o">},</span>
    <span class="s">&#34;transformer&#34;</span><span class="o">:</span> <span class="s">&#34;path&#34;</span>
