<td class="lntd">
<pre class="chroma"><code class="language-java" data-lang="java"><span class="o">{</span>
  <span class="s">&#34;picBed&#34;</span><span class="o">:</span> <span class="o">{</span>
    <span class="s">&#34;current&#34;</span><span class="o">:</span> <span class="s">&#34;gitee&#34;</span><span class="o">,</span>
    <span class="s">&#34;uploader&#34;</span><span class="o">:</span> <span class="s">&#34;gitee&#34;</span><span class="o">,</span>
    <span class="s">&#34;gitee&#34;</span><span class="o">:</span> <span class="o">{</span>
      <span class="s">&#34;branch&#34;</span><span class="o">:</span> <span class="s">&#34;master&#34;</span><span class="o">,</span>
      <span class="s">&#34;customPath&#34;</span><span class="o">:</span> <span class="s">&#34;yearMonth&#34;</span><span class="o">,</span>
      <span class="s">&#34;customUrl&#34;</span><span class="o">:</span> <span class="s">&#34;&#34;</span><span class="o">,</span>
