    <span class="s">&#34;lastSync&#34;</span><span class="o">:</span> <span class="s">&#34;2020-04-30 01:41:13&#34;</span>
  <span class="o">},</span>
  <span class="s">&#34;picgo-plugin-github-plus&#34;</span><span class="o">:</span> <span class="o">{</span>
    <span class="s">&#34;lastSync&#34;</span><span class="o">:</span> <span class="s">&#34;2020-04-07 11:09:08&#34;</span>
  <span class="o">}</span>
